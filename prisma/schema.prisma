model User {
  id              Int       @id @default(autoincrement())
  firstName       String
  lastName        String
  email           String
  password        String
  lastLogin       DateTime?
  lastUpdatedTime DateTime
  image           String
  role            Role
  approved        Boolean?
}

enum Role {
  READ_ONLY_ADMIN
  READ_WRITE_ADMIN
  USER
}
